<?xml version="1.0" encoding="UTF-8"?>
<!-- web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0" -->
<web-app>
    <!-- Default page to serve -->
   <welcome-file>
       SpaceApplication.html
   </welcome-file>
    <!-- Servlets -->
    <servlet>
        <servlet-name>SpaceApplicationService</servlet-name>
        <servlet-class>com.spaceApplication.server.space.SpaceApplicationServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SpaceApplicationService</servlet-name>
        <url-pattern>/SpaceApplication/SpaceApplicationService</url-pattern>
    </servlet-mapping>
</web-app>
